<CsoundSynthesizer>
<CsOptions>
-i{{ input }}
-o{{ output }} {{ flags }}
</CsOptions>
<CsInstruments>
sr = {{ sample_rate }}
ksmps = {{ ksmps }}
nchnls = 1
0dbfs = 1

{% for channel in channels %}
    chn_k "{{ channel.name }}", 3
    chnset {{ channel.value }}, "{{ channel.name }}" ; not sure if the channel should be set on startup
{% endfor %}

instr	1

{% for channel in channels %}
    k_{{ channel.name}} chnget "{{ channel.name }}"
{% endfor %}

    aIn in
    {{ effect }}
    {{ analyzer }}
    out aOut

endin

</CsInstruments>
<CsScore>
i 1 0 {{ duration }}
e
</CsScore>
</CsoundSynthesizer>